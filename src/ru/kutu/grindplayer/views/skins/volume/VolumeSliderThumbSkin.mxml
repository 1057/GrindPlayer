<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:helpers="ru.kutu.grindplayer.views.skins.helpers.*"
		>
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="up" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect
		width="5"
		height="14"
		>
		<s:fill>
			<s:SolidColor color="{getStyle('volumeThumbColor')}" />
		</s:fill>
	</s:Rect>
	
	<s:Rect
		y="14"
		width="5"
		height="1"
		>
		<s:fill>
			<s:SolidColor />
		</s:fill>
	</s:Rect>
	
</s:Skin>
